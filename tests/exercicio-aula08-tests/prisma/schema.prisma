generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:admin123@localhost:5432/ex08?schema=public"
}

model Reservation {
  id        Int      @id @default(autoincrement())
  startDate DateTime
  endDate   DateTime

  @@map("reservations")
}
